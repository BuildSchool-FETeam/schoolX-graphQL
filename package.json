{
  "name": "school-x-be",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist/*",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" ",
    "check-style": "prettier --check \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:local": "nest start --watch NODE_ENV=local",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint-report": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:all": "jest --ci --testResultsProcessor=jest-junit",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:cov": "jest --ci --changedSince=\"dev\" --coverage",
    "test:change": "jest --coverage --changeSince=origin/dev",
    "typeorm": "typeorm-ts-node-commonjs",
    "migrate:create": "typeorm migration:create -n",
    "migrate:gen": "yarn run typeorm -d ./data-source.ts migration:generate",
    "migrate:up": "yarn run typeorm migration:run -d ./data-source.ts",
    "migrate:down": "yarn run typeorm migration:revert -d ./data-source.ts",
    "start:docker": "./scripts/docker-start-app.sh",
    "start-prod:docker": "./scripts/docker-start-prod.sh",
    "migrate:docker": "rimraf src/migrations/* && npm run migrate:gen",
    "e2e:ci": "docker compose -f docker-compose.ci.yml up --build --abort-on-container-exit"
  },
  "dependencies": {
    "@apollo/gateway": "^2.2.1",
    "@google-cloud/storage": "^6.7.0",
    "@nestjs/apollo": "^10.1.6",
    "@nestjs/common": "^9.2.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "9.2.0",
    "@nestjs/graphql": "^10.1.6",
    "@nestjs/platform-express": "^9.2.0",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/terminus": "^9.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "@types/graphql-upload": "^8.0.12",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.168",
    "@types/react": "^18.2.0",
    "apollo-server-express": "^3.11.1",
    "axios": "^1.2.0",
    "bcrypt": "^5.1.0",
    "cache-manager": "^5.1.3",
    "express": "^4.18.2",
    "global": "^4.4.0",
    "graphql": "^16.6.0",
    "graphql-request": "^6.0.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-tools": "^8.3.12",
    "graphql-upload": "13.0.0",
    "install-peerdeps": "^3.0.3",
    "jimp": "^0.22.7",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "minimist": "^1.2.7",
    "netmask": "^2.0.2",
    "node-mailjet": "^6.0.2",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.7",
    "ts-morph": "^17.0.1",
    "tslint": "^6.1.3",
    "typeorm": "^0.3.10",
    "xmlbuilder": "^15.1.1"
  },
  "devDependencies": {
    "@eslint/create-config": "0.4.1",
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.2.0",
    "@types/bcrypt": "^3.0.0",
    "@types/cache-manager": "^3.4.0",
    "@types/express": "^4.17.8",
    "@types/jest": "^27.0.3",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "eslint": "^8.28.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^29.3.1",
    "jest-junit": "^15.0.0",
    "madge": "^5.0.1",
    "prettier": "^2.8.0",
    "run-script-webpack-plugin": "^0.1.1",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-node-externals": "^3.0.0",
    "yarn-audit-fix": "^9.3.10"
  },
  "jest": {
    "setupFilesAfterEnv": ["<rootDir>/src/common/setupTests.ts"],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "roots": [
      "src"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!src/common/mock/*.ts",
      "!src/migrations/*.ts",
      "!**/*.module.ts"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "src/(.*)": "<rootDir>/src/$1"
    },
    "coverageReporters": [
      "text",
      "json",
      "html",
      "json-summary",
      "lcov"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/src/common/mock/"
    ]
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "outputDirectory": ".",
    "outputName": "report.xml",
    "uniqueOutputName": "false",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  }
}
